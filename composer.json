{
    "name": "codesinging/component-builder",
    "description": "A package to build Vue UI component for PHP development",
    "license": "MIT",
    "authors": [
        {
            "name": "codesinging",
            "email": "codesinging@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "codesinging/helpers": "^1.0"
